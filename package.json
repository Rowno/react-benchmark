{
  "name": "react-benchmark",
  "version": "2.0.0",
  "description": "A tool for benchmarking the render performance of React components.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "react",
    "benchmark",
    "chrome",
    "webpack",
    "production"
  ],
  "repository": "Rowno/react-benchmark",
  "license": "ISC",
  "main": "./lib/index.js",
  "bin": {
    "react-benchmark": "./lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "xo && ava",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "babel-loader": "^8.0.2",
    "benchmark": "^2.1.4",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "get-port": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "humanize-number": "^0.0.2",
    "lodash": "^4.17.10",
    "meow": "^5.0.0",
    "ora": "^3.0.0",
    "pkg-dir": "^3.0.0",
    "pluralize": "^7.0.0",
    "puppeteer": "^1.7.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "resolve-from": "^4.0.0",
    "source-map-loader": "^0.2.4",
    "tempy": "^0.2.1",
    "webpack": "^4.17.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "ava": "^0.25.0",
    "execa": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "xo": "^0.23.0"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "prettier": true,
    "envs": [
      "node",
      "browser"
    ]
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ]
  }
}

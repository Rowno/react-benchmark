{
  "name": "react-benchmark",
  "version": "0.0.0",
  "description": "Benchmarks the render performance of React components.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [],
  "repository": "Rowno/react-benchmark",
  "license": "ISC",
  "private": true,
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "test": "xo",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": ["prettier"],
    "space": true,
    "semicolon": false,
    "envs": ["node", "browser"],
    "rules": {
      "indent": ["off"]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": ["xo --fix", "prettier --write", "git add"],
    "*.{json,md}": ["prettier --write", "git add"]
  }
}

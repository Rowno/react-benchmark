{
  "name": "react-benchmark",
  "version": "1.0.4",
  "description": "A tool for benchmarking the render performance of React components.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "react",
    "benchmark",
    "chrome",
    "webpack",
    "production"
  ],
  "repository": "Rowno/react-benchmark",
  "license": "ISC",
  "main": "./lib/index.js",
  "bin": {
    "react-benchmark": "./lib/cli.js"
  },
  "files": [
    "lib",
    ".babelrc"
  ],
  "scripts": {
    "test": "xo && ava",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "benchmark": "^2.1.4",
    "express": "^4.16.2",
    "get-port": "^3.2.0",
    "html-webpack-plugin": "^2.30.1",
    "humanize-number": "^0.0.2",
    "lodash": "^4.17.5",
    "meow": "^4.0.0",
    "ora": "^1.3.0",
    "pluralize": "^7.0.0",
    "puppeteer": "^1.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sander": "^0.6.0",
    "tempy": "^0.2.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-preset-react": "^6.24.1",
    "execa": "^0.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "prettier": true,
    "envs": [
      "node",
      "browser"
    ]
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ]
  }
}

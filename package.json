{
  "name": "react-benchmark",
  "version": "1.0.4",
  "description":
    "A tool for benchmarking the render performance of React components.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": ["react", "benchmark", "chrome", "webpack", "production"],
  "repository": "Rowno/react-benchmark",
  "license": "ISC",
  "bin": {
    "react-benchmark": "./lib/cli.js"
  },
  "files": ["lib", ".babelrc"],
  "scripts": {
    "test": "xo",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "benchmark": "^2.1.4",
    "express": "^4.16.2",
    "get-port": "^3.2.0",
    "humanize-number": "^0.0.2",
    "lodash": "^4.17.4",
    "meow": "^4.0.0",
    "ora": "^1.3.0",
    "pluralize": "^7.0.0",
    "puppeteer": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sander": "^0.6.0",
    "tempy": "^0.2.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": ["prettier"],
    "space": true,
    "semicolon": false,
    "envs": ["node", "browser"],
    "rules": {
      "indent": ["off"]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": ["xo --fix", "prettier --write", "git add"],
    "*.{json,md}": ["prettier --write", "git add"]
  }
}
